/* 0x01000004 - other
   0x01000005 - zero
   0x01000006 - one
   0x01000007 - two
   0x01000008 - few
   0x01000009 - many */
    <plurals name="adblock_rules_number_text">
        <item quantity="other">%d items blocked</item>
        <item quantity="one">%d item blocked</item>
    </plurals>
    <plurals name="find_on_page_matches">
        <item quantity="other">%d matches</item>
        <item quantity="one">%d match</item>
    </plurals>
    <plurals name="last_num_days">
        <item quantity="other">Last %d days</item>
    </plurals>
    <plurals name="matches_found">
        <item quantity="other">%1$d of %2$d</item>
        <item quantity="one">1 match</item>
    </plurals>
    <plurals name="video_download_clear_info">
        <item quantity="other">Removed %1$d videos successfully.</item>
        <item quantity="one">Removed %1$d video successfully.</item>
    </plurals>
    <plurals name="video_download_clear_prompt">
        <item quantity="other">Clear %1$d videos?</item>
        <item quantity="one">Clear %1$d video?</item>
    </plurals>
    <plurals name="video_history_clear_info">
        <item quantity="other">%1$d items deleted from history.</item>
        <item quantity="one">%1$d item deleted from history.</item>
    </plurals>
    <plurals name="video_history_clear_prompt">
        <item quantity="other">Delete %1$d items from history?</item>
        <item quantity="one">Delete %1$d item from history?</item>
    </plurals>
    <plurals name="video_subtitle_last_message">
        <item quantity="other">%d minutes remaining</item>
        <item quantity="one">1 minute remaining</item>
    </plurals>
